{
  "name": "@maxmilton/stylelint-config",
  "version": "0.0.1",
  "description": "Stylelint configuration with support for XCSS via ekscss",
  "repository": "maxmilton/stylelint-config",
  "author": "Max Milton <max@happycode.ltd>",
  "license": "MIT",
  "main": "stylelint.config.js",
  "scripts": {
    "lint": "pnpm run lint:js && pnpm run lint:ts",
    "lint:js": "eslint --ignore-path .gitignore --ext .cjs,.js,.mjs,.ts .",
    "lint:ts": "tsc",
    "test": "uvu -r tsm test '\\.test\\.ts$'"
  },
  "peerDependencies": {
    "stylelint": "^14.0.0"
  },
  "dependencies": {
    "postcss-ekscss": "0.0.1",
    "stylelint-order": "5.0.0"
  },
  "devDependencies": {
    "@types/node": "16.11.11",
    "@typescript-eslint/eslint-plugin": "5.5.0",
    "@typescript-eslint/parser": "5.5.0",
    "eslint": "8.4.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "16.1.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-unicorn": "39.0.0",
    "prettier": "2.5.1",
    "stylelint": "14.1.0",
    "tsm": "2.2.1",
    "typescript": "4.5.2",
    "uvu": "0.5.2"
  }
}
