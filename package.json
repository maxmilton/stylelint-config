{
  "name": "@maxmilton/stylelint-config",
  "version": "0.0.12",
  "description": "Stylelint configuration with support for XCSS via ekscss",
  "repository": "maxmilton/stylelint-config",
  "author": "Max Milton <max@happyhackers.xyz>",
  "license": "MIT",
  "main": "stylelint.config.js",
  "scripts": {
    "lint": "bun run lint:js && bun run lint:ts",
    "lint:js": "eslint --ignore-path .gitignore --ext .cjs,.js,.mjs,.ts .",
    "lint:ts": "tsc --noEmit",
    "_test": "bun test",
    "test": "node -r uvu ./test/node.test.mjs"
  },
  "peerDependencies": {
    "stylelint": "^15.0.0"
  },
  "dependencies": {
    "postcss-ekscss": "^0.0.9",
    "stylelint-order": "^6.0.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "bun-types": "1.0.1",
    "eslint": "8.49.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-unicorn": "48.0.1",
    "prettier": "3.0.3",
    "stylelint": "15.10.3",
    "stylelint-config-standard": "34.0.0",
    "typescript": "5.2.2",
    "uvu": "0.5.6"
  }
}
